{
  "name": "api-data",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16 || <=18"
  },
  "author": "Lawrence Good",
  "description": "",
  "scripts": {
    "start": "node build/main.js",
    "build": "ts-node scripts/build.ts",
    "dev": "cross-env NODE_ENV=development node -r ts-node/register/transpile-only scripts/start.ts",
    "lint": "eslint src/**/*.{js,ts} --cache",
    "lint:fix": "eslint --cache --fix src/**/*.{js,ts}",
    "typecheck": "npx tsc --noEmit"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": "eslint --cache --fix",
    "config/**/*.{js,ts}": "eslint --cache --fix",
    "scripts/**/*.{js,ts}": "eslint --cache --fix"
  },
  "nodemonConfig": {
    "ts": "ts-node",
    "watch": [
      "./src/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/toomuchrice4u/api-data.git"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "dayjs": "^1.11.5",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.249",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@types/progress-bar-webpack-plugin": "^2.1.2",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.6.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3",
    "webpack": "^5.68.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.11.0",
    "webpack-node-externals": "^3.0.0"
  }
}
